<h1>Pages#home</h1>
<% p 'logged in user' %>
<% p @logged_in_user %>
<% if @logged_in_user.nil? %>
  <h2>Sign in</h2>
  <li><%= link_to("Login", controller="sessions", action: "new") %></li>
<% else %>
  <h2> Welcome back, <%= @logged_in_user.name %></h2>
  <li><%= link_to("Logout", controller="sessions", :method => :delete) %></li>
<% end %>
